{
  "name": "CityLights",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0e17",
    "editor.foreground": "#c7d9e6",
    "editorCursor.foreground": "#5bc0de",
    "editor.lineHighlightBackground": "#1c2531",
    "editorLineNumber.foreground": "#5e7184",
    "editor.selectionBackground": "#285773",
    "editor.selectionHighlightBackground": "#376b89",
    "activityBar.background": "#0a0e17",
    "activityBar.foreground": "#5bc0de",
    "activityBarBadge.background": "#00bfff",
    "sideBar.background": "#0d141c",
    "sideBar.foreground": "#c7d9e6",
    "statusBar.background": "#1c2531",
    "statusBar.foreground": "#5bc0de",
    "titleBar.activeBackground": "#1c2531",
    "titleBar.activeForeground": "#c7d9e6"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5e7184",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#5bc0de",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#87cefa",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c7d9e6",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#00bfff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#87cefa",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#7ab5e7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#c7d9e6"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#5bc0de",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ffcc99",
        "fontStyle": "normal"
      }
    }
  ]
}
